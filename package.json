{
	"name": "svg-sprite-generator",
	"publisher": "smatDnepr",
	"displayName": "SVG Sprite Generator",
	"description": "Generate svg-sprite from many svg-files",
	"icon": "images/logoSmatDnepr.jpg",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.57.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:smat.svgSpriteGenerate"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "smat.svgSpriteGenerate",
				"title": "SVG Sprite Generate"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "explorerResourceIsFolder",
					"command": "smat.svgSpriteGenerate"
				}
			]
		},
		"configuration": {
			"title": "SVG Sprite Generator",
			"properties": {
				"svgSpriteGenerator.outputFileName": {
					"type": ["string", "null"],
					"default": "_sprite",
					"description": "SVG sprite output filename without extension, eg: '_sprite' or 'sprite'"
				}
			}
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js",
		"dev": "webpack --mode development",
		"prod": "webpack --mode production",
		"watch": "webpack --mode development --watch",
		"test-compile": "tsc -p ./"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/vscode": "^1.57.0",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2",
		"webpack": "^5.39.1",
		"webpack-cli": "^4.7.2"
	},
	"dependencies": {}
}
